		name: white
		color: {
			r: 0.2
			g: 0.2
			b: 0.2
			a: 1
		}
	}
}
com.badlogic.gdx.scenes.scene2d.ui.Button$ButtonStyle: {
	default: {
		up: button
		down: button-down
	}
	music: {
		up: music-off
		down: music-down
		checked: music
	}
	left: {
		up: left-button
		down: left-button-down
	}
	sound: {
		up: sound-off
		down: sound-down
		checked: sound
	}
	toggle: {
		up: toggle-button-off
		down: toggle-button-down
		checked: toggle-button
	}
	right: {
		up: right-button
		down: right-button-down
	}
}
com.badlogic.gdx.scenes.scene2d.ui.CheckBox$CheckBoxStyle: {
	default: {
		checkboxOn: check-on
		checkboxOff: check-off
		font: font-label
	}
	switch: {
		checkboxOn: switch
		checkboxOff: switch-off
		font: font-label
	}
}
com.badlogic.gdx.scenes.scene2d.ui.ImageButton$ImageButtonStyle: {
	default: {
		up: button
		down: button-down
	}
}
com.badlogic.gdx.scenes.scene2d.ui.Label$LabelStyle: {
	default: {
		font: font-label
	}
